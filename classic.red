    transition: 0.25s ease;
}
.subheadline.m13 {
    text-align: center;
}
.headline>a:before, .midheadline>a:before, .subheadline>a:before {
    display: none;
}
.headline, .midheadline, .subheadline {
    border-left: none;
    border-bottom: 1px solid #e2e0e0;
    background: #fff;
}
.headline, .midheadline, .subheadline:hover {
    border-bottom: 1px solid #cc4b4b;
}
.headline>a, .midheadline>a, .subheadline>a {
    color: #000;
    text-decoration: none;
}
.headline>a:hover, .midheadline>a:hover, .subheadline>a:hover {
    color: #000;
    text-decoration: none;
}
.p-profiles .profile-head .c-history .entry {
