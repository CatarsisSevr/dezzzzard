::-webkit-scrollbar-button {
  background-image: url(''); 
  background-repeat: no-repeat;
  width: 0px;
  height: 0px
}
::-webkit-scrollbar-track {
  background-color: rgba(0, 0, 0, 0.1)
}
::-webkit-scrollbar-thumb {
  -webkit-border-radius: 0px;
  background-color: #cc4b4b;
}
::-webkit-scrollbar-thumb:hover {
  background-color: #cc4b4b;
}
::-webkit-scrollbar-corner {
  background-repeat: no-repeat;
}
::-webkit-resizer {
  background-image: url('');
  background-repeat: no-repeat;
  width: 6px;
  height: 6px;
}
::-webkit-scrollbar {
  width: 6px;
  height: 6px;
}
.l-top_menu .inner {
  background-color: #cc4b4b;
  color: #fff;
  box-shadow: 0 0px 8px 0px rgba(0, 0, 0, 0.25);
}
.l-top_menu {
  background: none;
  border-bottom: none;
}
.l-top_menu .menu-items>.entry {
  color: #ffffff;
}
.b-options-floated.mylist.mobile-desktop a::after,
.b-options-floated.mylist.mobile-desktop .link::after {
  display: none;
}
.p-user_rates-index .l-content .b-options-floated span::before,
.p-user_rates-index .l-content .b-options-floated span::after {
  display: none;
}
.b-options-floated.mobile-desktop {
    position: inherit;
    text-align: center;
    z-index: 10;
}
.block.avatar {
    margin-top: 4rem;
} 
.b-options-floated.mobile-desktop a {
    display: initial;
    text-align: center;
    background: #fff;
    border: 1px solid #ddd;
    color: #000;
    padding: 0.5rem 0.6rem;
    margin: 0.2rem 0.6rem;
    border-radius: 2px;
    text-decoration: none;
    transition: 0.3s ease;
}
.b-options-floated.mobile-desktop a:hover {
    border: 1px solid #e61953;
    color: #e61953;
}
/* Нажатая кнопка */
.b-options-floated.mobile-desktop a.selected {
    background: #cc4b4b;
    color: #fff;
    border: 1px solid #461623;
}

/* Наводка на нажатую кнопку */
.b-options-floated.mobile-desktop a.selected:hover {
    background: #fff;
    color: #e61953;
    border: 1px solid #cc4b4b;
}

/* Цифры */
.p-user_rates-index .l-content .b-options-floated span {
    display: inline;
    border-left: 1px solid #ddd;
    padding: 2px 6px;
    white-space: normal;
}

/* Кнопка "свернуть" */
.p-user_rates-index .l-content .b-options-floated span[class*="action"] {
  color: #000;
  background: #fff;
  margin: 0;
  display: inline;
  border: 1px solid #ddd;
  border-radius: 2px;
  padding: 5px 10px;
  white-space: normal;
  transition: 0.2s ease;
}
.p-user_rates-index .l-content .b-options-floated span[class*="action"]:hover {
  color: #e61953;
  border-color: #e61953;
}
@media (max-width: 860px) {
  .b-options-floated.mobile-desktop a {
    display: inline-block;
  }
}
.b-options-floated.mobile-desktop {
    margin-bottom: 15px;
}
.p-user_rates-index .list-lines tr .name a {
    color: #cc4b4b;
}
.p-user_rates-index .list-lines tr .name a:hover {
    color: #cc4b4b;
    text-decoration: none;
}
.p-user_rates-index .list-lines tr td .rewatches:not(:empty) {
    text-transform: uppercase;
    display: inline-block;
    vertical-align: text-top;
    font-size: 10px;
    color: hsla(0, 0%, 70%, 1);
    border: 1px solid #ddd;
    padding: 1px 4px;
    margin: 1px 10px;
    border-radius: 2px;
}
.b-main_search ul.popup {
  display: none;
}
