.b-stats_bar .bar {
  border-radius: 0;
  margin-bottom: 2px;
  height: 10px;
}
.p-profiles .profile-head .c-info .c-lists-info .b-stats_bar.manga {
  margin-top: 1.2rem;
}
.p-profiles-show .b-stats_bar.lifetime .bar {
  height: 8px;
  border-radius: 0;
}
.b-stats_bar .bar .first,
.b-stats_bar .bar .second,
.b-stats_bar .bar .third {
  font-size: 0;
  line-height: 2.25rem;
}
.p-profiles-show .b-stats_bar.anime .bar,
.p-profiles-show .b-stats_bar.manga .bar {
  display: initial;
}
.p-profiles-show .b-stats_bar.anime .bar .first {
    background-image: linear-gradient(22deg, #cc4b4b, #e07676);
}
.p-profiles-show .b-stats_bar.anime .bar .second {
    background-image: linear-gradient(22deg, #ededed, #cccccc);
}
.p-profiles-show .b-stats_bar.anime .bar .third {
    background-image: linear-gradient(22deg, #cccccc, #e6e6e6);
}
.p-profiles-show .b-stats_bar.manga .bar .first {
    background-image: linear-gradient(22deg, #cc4b4b, #e07676);
}
.p-profiles-show .b-stats_bar.manga .bar .second {
    background-image: linear-gradient(22deg, #ededed, #cccccc);
}
.p-profiles-show .b-stats_bar.manga .bar .third {
    background-image: linear-gradient(22deg, #cccccc, #e6e6e6);
}
.p-profiles-show .b-stats_bar.lifetime .bar .first {
    border-radius: 0;
    background-image: linear-gradient(22deg, #cc4b4b, #e07676);
}
.b-stats_bar .bar .third {
    background-image: linear-gradient(22deg, #ededed, #cccccc);
}
.graph.vertical.x34 .line {
  margin-right: calc(100% - 8px) / 33) - 8px);
}
.graph.vertical .line .bar-container .bar {
  width: 8px;
  border-radius: 0;
}
.bar.vertical .line .bar-container .value,
.bar.vertical .line .bar-container .value.mini {
  font-size: 0;
}
.line:nth-child(odd) > .bar-container > .bar,
.line:nth-child(odd) > .bar-container > .bar:hover,
.line:nth-child(even) > .bar-container > .bar,
.line:nth-child(even) > .bar-container > .bar:hover,
.graph.vertical:not(:hover) .line:last-child .bar-container .bar,
.graph.vertical:hover .line:last-child .bar-container .bar{
  background: linear-gradient(0deg, #cc4b4b, #e07676);
}
.p-profiles-show .b-stats_bar.anime a.title,
.p-profiles-show .b-stats_bar.manga a.title {
  display: inline-block !important;
  vertical-align: top !important;
  min-width: 92px !important;
  padding: .375rem 1rem !important;
  margin-left: 0 !important;
  background: #fff !important;
  color: #000 !important;
  font-weight: bold !important;
  line-height: 1.5rem !important;
}
.p-profiles-show .b-stats_bar.anime a.title:hover {
    text-decoration: none;
}
.p-profiles-show .b-stats_bar.manga a.title:hover {
    text-decoration: none;
}
.p-profiles .profile-head .c-brief header h1 {
    font-family: "IBM Plex Sans" !important;
    font-weight: bold !important;
}
.p-profiles .profile-head .c-brief header h1 {
